pragma solidity ^0.8.0;
contract math{
    function addition (int a, int b ) public pure returns (int){
       return int(a+b);

    }

    function substraction (int a, int b) public pure returns (int){
        return int(a*b);

    }

    function multiplication (int a, int b) public pure returns (int){
        return int(a*b);

    }

    function divivision (int a, int b) public pure returns(int){
        return int(a/b);
    }
}
